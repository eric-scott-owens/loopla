{
  "name": "loopla-frontend",
  "version": "1.0.0",
  "description": "React frontend for Loopla",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run django\" \"npm run dev\" \"npm run redis\" \"npm run rqworker\"",
    "taskrunner": "concurrently \"npm run redis\" \"npm run rqworker\"",
    "django": "python manage.py runserver --settings prototype_1.settings_development",
    "redis": "redis-server /usr/local/etc/redis.conf",
    "rqworker": "python manage.py rqworker",
    "socket-io": "cd socket-app; node index.js",
    "dev": "webpack --config webpack.dev.config.js --mode development ./frontend_v1/src/index.js --watch",
    "dev-poll": "webpack --config webpack.dev.config.js --mode development ./frontend_v1/src/index.js --watch-poll --watch",
    "build": "webpack --config webpack.build.config.js --mode production ./frontend_v1/src/index.js",
    "heroku-postbuild": "webpack --config webpack.prod.config.js --mode production ./frontend_v1/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cordova-copy-app-resources": "cd appWebView; cd www; rm -rf frontend_v1; cd ..; cd ..; yes | cp -rf frontend_v1/static/frontend_v1 appWebView/www/frontend_v1",
    "cordova-initialize-platforms": "cd appWebView; cordova platform add android; cordova platform add ios; cd ..;",
    "cordova-emulate-android": "npm run cordova-initialize-platforms; npm run cordova-copy-app-resources; cd appWebView; cordova emulate android; cd ..;",
    "cordova-run-android": "npm run cordova-initialize-platforms; npm run cordova-copy-app-resources; cd appWebView; cordova run android; cd ..;",
    "cordova-build-android": "npm run cordova-initialize-platforms; webpack --config webpack.prod.config.js --mode production ./frontend_v1/src/index.js; npm run cordova-copy-app-resources; npm run cordova-copy-app-resources; cd appWebView; cordova build android; cd ..;"
  },
